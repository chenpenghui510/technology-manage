<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.system.mapper.SysUserCompanyDiscussMapper">
    <select id="pageListNew" resultType="org.jeecg.modules.system.entity.SysUserCompanyDiscuss">
        select * from sys_user_company_discuss
        where 1=1
        <if test="name!=null and name!=''">
            AND name LIKE concat('%',#{name},'%')
        </if>
        <if test="sort!=null and sort!=''">
            AND sort = #{sort}
        </if>
        <if test="level!=null and level!=''">
            AND level = #{level}
        </if>
        <if test="startTime!=null ">
            and patent_time &gt;=  #{startTime}
        </if>
        <if test="endTime!=null ">
            and patent_time &lt;=  #{endTime}
        </if>
        <if test="userId!=null and userId!=''">
            AND userId = #{userId}
        </if>
    </select>
</mapper>