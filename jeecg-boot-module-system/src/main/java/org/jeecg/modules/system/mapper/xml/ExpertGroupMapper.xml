<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.system.mapper.ExpertGroupMapper">

    <select id="findByName" resultType="org.jeecg.modules.system.entity.ExpertGroup">
      SELECT * FROM expert_group
      WHERE group_name = #{groupName} and del_flag=0
    </select>
    
</mapper>